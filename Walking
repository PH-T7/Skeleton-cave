extends CharacterBody2D

var health













func _process(delta):
	if Input.is_action_pressed("ui_left"):
		global_position.x -= 3
	elif Input.is_action_pressed("ui_right"):
		global_position.x += 3
	
	
	if Input.is_action_pressed("ui_up"):
		global_position.y -= 3
	elif Input.is_action_pressed("ui_down"):
		global_position.y += 3

func _input(event):
	
	if event.is_action_pressed("movimentacao"):
		print("ph")

